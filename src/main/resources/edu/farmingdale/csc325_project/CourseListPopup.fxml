<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.farmingdale.csc325_project.CourseListPopupController">
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <center>
      <TableView fx:id="tableView_popup" BorderPane.alignment="CENTER">
        <columns>
            <TableColumn fx:id="subjectCol" minWidth="-1.0" prefWidth="75.0" text="Subject" />
          <TableColumn fx:id="codeCol" minWidth="-1.0" prefWidth="75.0" text="Code" />
            <TableColumn fx:id="titleCol" minWidth="-1.0" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="CRNCol" minWidth="-1.0" prefWidth="75.0" text="CRN" />
        </columns>
      </TableView>
   </center>
</BorderPane>
